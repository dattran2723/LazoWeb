<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/img/logo.png" type="image/png" rel="shortcut icon" />
    <title>@ViewBag.Title Lazo</title>
    @*link css bootstrap*@
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font/css/all.css" rel="stylesheet" />
    @*css slide*@
    <link href="~/Content/css/slide.css" rel="stylesheet" />
    @*css informationsLazo*@
    <link href="~/Content/css/informationsLazo.css" rel="stylesheet" />
    @*css typicalkhachhang*@
    <link href="~/Content/css/typicalkhachhang.css" rel="stylesheet" />
    @*css customers feedback and download app*@
    <link href="~/Content/css/StyleFeedback.css" rel="stylesheet" />
    @*css footer*@
    <link href="~/Content/css/footer.css" rel="stylesheet" />
    <link href="~/Content/css/menu.css" rel="stylesheet" />
    @*jquery*@
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div>
        @Html.Partial("_MenuHeader")
        @RenderBody()
    </div>
    @Html.Partial("/Views/Shared/_FooterPartial.cshtml")
    @*script*@
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/feedback.js"></script>
    @*js croll to id*@
    <script src="~/Scripts/js/crolltoid.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
